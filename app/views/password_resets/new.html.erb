<h1>Forgot password</h1>
<%= form_for :password_set, url: :password_resets do |f| %>
<p>
<%= f.label :email %>
<%= f.text_field :email %>
</p>

<p>
<%= f.label :password %>
<%= f.password_field :password %>
</p>

<p>
<%= f.label :password_confirmation, "Confirmation" %>
<%= f.password_field :password_confirmation %>
</p>

<br><%= f.submit "Submit" %></br>
<% end %>
